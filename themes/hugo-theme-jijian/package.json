{
  "name": "hugo-theme-jijian",
  "version": "0.0.1",
  "description": "Hugo jijian theme",
  "dependencies": {
    "bulma": "^0.9.2",
    "highlight.js": "^10.6.0",
    "katex": "^0.12.0",
