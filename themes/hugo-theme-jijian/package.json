    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-webpack-plugin": "^2.2.1",
    "file-loader": "^6.1.1",
    "gray-matter": "^4.0.2",
    "json2yaml": "^1.1.0",
    "line-column": "^1.0.2",
    "mini-css-extract-plugin": "^1.2.0",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "scss": "^0.2.4",
    "scss-loader": "^0.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.9",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "watch": "^1.0.2",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:css build:js",
    "build:css": "node-sass --output-style compressed src/scss/index.scss assets/css/main.css",
    "build:js": "webpack",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "watch": "watch 'npm run build' src",
    "gen": "node ./src/index.js"
